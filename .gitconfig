[user]
    email = cw@reosource.io
    name = Cameron Wong
[push]
    default = simple
    autoSetupRemote = true
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    sw = switch
    sub = submodule
    down = "!git remote update -p; git merge --ff-only @{u}"
    up = push
    sdiff = "diff --cached"
    aa = "add -A"
    cs = "!git aa; git commit -m 'Update.'; git up"
    pom = "!git push -u origin master"
[core]
    excludesfile = ~/.gitignore
    autocrlf = input
